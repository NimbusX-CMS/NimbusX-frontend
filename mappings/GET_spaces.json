{
  "mappings": [
    {
      "scenarioName": "Unauthorized get space",
      "requiredScenarioState": "Started",
      "request": {
        "method": "GET",
        "url": "/spaces"
      },
      "response": {
        "status": 401,
        "headers": {
          "Content-Type": "application/json"
        }
      }
    },
    {
      "scenarioName": "Get space",
      "requiredScenarioState": "Started",
      "request": {
        "method": "GET",
        "url": "/spaces",
        "cookies": {
          "session": {
            "matches": "supersecretusersessionkey"
          }
        }
      },
      "response": {
        "status": 200,
        "jsonBody": [
          {
            "id": 0,
            "name": "Sample Webseite",
            "color1": "#576CA8",
            "color2": "#274690",
            "color3": "#1D1F25",
            "color4": "#272A30",
            "primaryLanguage": "de-DE",
            "languages": [
              "de-DE",
              "en-US"
            ]
          }
        ],
        "headers": {
          "Content-Type": "application/json"
        }
      }
    }
  ]
}