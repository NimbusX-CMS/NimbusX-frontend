{
  "mappings": [
    {
      "scenarioName": "Invalid login",
      "requiredScenarioState": "Started",
      "request": {
        "method": "POST",
        "url": "/login"
      },
      "response": {
        "status": 401,
        "headers": {
          "Content-Type": "application/json"
        }
      }
    },
    {
      "scenarioName": "Login",
      "requiredScenarioState": "Started",
      "request": {
        "method": "POST",
        "url": "/login",
        "bodyPatterns": [
          {
            "equalToJson": {
              "email": "max.mustermann@mail.de",
              "password": "password"
            }
          }
        ]
      },
      "response": {
        "status": 200,
        "jsonBody": {
          "session": "supersecretusersessionkey"
        },
        "headers": {
          "Content-Type": "application/json"
        }
      }
    }
  ]
}